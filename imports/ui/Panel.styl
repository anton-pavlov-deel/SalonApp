.panel
  text-decoration none
  outline none
  display inline-block
  line-height 45px
  border-radius 20px
  margin 10px 20px
  font-family 'Montserrat', sans-serif
  font-size 11px
  text-transform uppercase
  text-align center
  letter-spacing 3px
  font-weight 600
  color #524f4e
  background white
  box-shadow 0 8px 15px rgba(0,0,0,0.1)
  transition .3s
